{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"doctrine/dbal": "2.5.13",
		"guzzlehttp/guzzle": "^5.3",
		"intervention/image": "^2.7",
		"laravel/framework": "5.2.*",
		"maxmind-db/reader": "1.4.1",
		"namshi/jose": "7.0.*",
		"symfony/lock": "^3.4",
		"tymon/jwt-auth": "^1.0"
	},
	"require-dev": {
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"platform": {
			"php": "7.0.33",
			"ext-mcrypt": "1",
			"ext-mbstring": "1",
			"ext-xml": "1",
			"ext-xmlwriter": "1",
			"ext-dom": "1",
			"ext-bcmath": "1",
			"ext-gd": "1",
			"ext-simplexml": "1",
			"ext-xmlreader": "1",
			"ext-zip": "1"
		},
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	}
}
